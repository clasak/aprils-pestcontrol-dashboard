{
  "name": "@pestcontrol/backend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/websockets": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.0.0",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@sendgrid/mail": "^7.7.0",
    "typeorm": "^0.3.17",
    "pg": "^8.11.0",
    "redis": "^4.6.0",
    "bull": "^4.11.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "bcrypt": "^5.1.0",
    "class-validator": "^0.14.0",
    "class-transformer": "^0.5.0",
    "axios": "^1.5.0",
    "@nestjs/swagger": "^7.1.0",
    "compression": "^1.7.4",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "nodemailer": "^6.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^3.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "typescript": "^5.2.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config ./test/jest-integration.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate": "npm run typeorm -- migration:run -d src/config/database.ts",
    "migrate:revert": "npm run typeorm -- migration:revert -d src/config/database.ts",
    "migrate:show": "npm run typeorm -- migration:show -d src/config/database.ts",
    "migration:create": "npm run typeorm -- migration:create src/database/migrations/$npm_config_name",
    "migration:generate": "npm run typeorm -- migration:generate src/database/migrations/$npm_config_name -d src/config/database.ts",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/seed.ts"
  }
}
